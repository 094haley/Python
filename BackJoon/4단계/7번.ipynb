    "\n",
    "\n",
    "score = list(map(int, input().split()))\n",
    "M = 0\n",
    "\n",
    "for i in score:\n",
    "    if M < i:\n",
    "        M = i\n",
    "\n",
    "print(score)\n",
    "print(M)\n",
    "\n",
    "def newscore(n):\n",
    "    n =  n / M * 100\n",
    "    return n\n",
    "\n",
    "score = list(map(newscore, score))\n",
    "\n"
